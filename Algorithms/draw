#
for (( N = 0; N < 250; N=N+1 ))
do
  file=$( printf '%03d' $N )
  cnt=`ls ${file}*.cc 2>/dev/null | wc -l` 
  if [ $cnt -ne 0 ]; then
    echo -en "X"
  else 
    echo -en " "
  fi
  if [ $((N%10)) -eq 0 ]; then
    echo -en "\n"
    printf '%03d' $((N+1))
    echo -en "  "
  fi
done
echo -en "\n"
